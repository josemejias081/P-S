<div class="row mt-3 mb-3 ">
  <% @businesses.each do |business| %>
    <% if business.featured? %>
      <div class="col-md-3 mb-2">
        <%= link_to business, class: "card-conten" do %>
          <%= image_tag business.logo, class: "rounded size-logo"%>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>
